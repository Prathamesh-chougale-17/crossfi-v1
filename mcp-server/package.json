{
  "name": "jeu-plaza-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for Jeu Plaza game management",
  "main": "dist/index.js",
  "bin": {
    "jeu-plaza-mcp-server": "dist/index.js"
  },
  "type": "module",
  "keywords": ["mcp", "model-context-protocol", "jeu-plaza", "games", "web3"],
  "author": "Jeu Plaza Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/jeu-plaza.git",
    "directory": "mcp-server"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "test": "node test-server.js",
    "health": "./health-check.sh",
    "deploy": "./deploy.sh",
    "publish": "./publish.sh",
    "prepublishOnly": "npm run build",
    "docker:build": "docker build -t jeu-plaza-mcp-server .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f mcp-server"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.4.0",
    "dotenv": "^16.4.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "typescript": "^5.3.0",
    "tsx": "^4.7.0"
  }
}