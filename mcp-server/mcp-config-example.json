{
  "mcpServers": {
    "jeu-plaza": {
      "command": "npx",
      "args": ["jeu-plaza-mcp-server@latest"],
      "env": {
        "API_BASE_URL": "http://localhost:3000",
        "API_KEY": "your-secure-api-key-here"
      },
      "disabled": false,
      "autoApprove": [
        "list_games",
        "get_game"
      ]
    }
  }
}